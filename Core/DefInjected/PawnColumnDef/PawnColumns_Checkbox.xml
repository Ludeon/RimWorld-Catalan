<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: Follow master while the master is drafted. -->
  <FollowDrafted.headerTip>Segueix l'amo quan està reclutat.</FollowDrafted.headerTip>
  
  <!-- EN: Follow master while the master is doing field work (hunting or taming animals). -->
  <FollowFieldwork.headerTip>Segueix l'amo quan fa tasques fora de la llar (caçant or domant animals).</FollowFieldwork.headerTip>
  
  <!-- EN: Mark animals to be hunted by your hunters. -->
  <Hunt.headerTip>Marca els animals per ser caçats pels vostres caçadors.</Hunt.headerTip>
  
  <!-- EN: Release to wild -->
  <ReleaseAnimalToWild.headerTip>Allibera a la natura</ReleaseAnimalToWild.headerTip>
  
  <!-- EN: Slaughter -->
  <Slaughter.headerTip>Escorxa</Slaughter.headerTip>
  
  <!-- EN: Tame a wild animal. -->
  <Tame.headerTip>Domestica un animal salvatge.</Tame.headerTip>
  
</LanguageData>